#: admin.py:15
msgid "Accept selected friendship requests"
msgstr "Seçilen arkadaşlık taleplerini kabul et"

#: admin.py:22
msgid "Decline selected friendship requests"
msgstr "Seçilen arkadaşlık taleplerini reddet"

#: admin.py:29
msgid "Cancel selected friendship requests"
msgstr "Seçilen arkadaşlık taleplerini iptal et"

#: models.py:18
msgid "friendship request"
msgstr "arkadaşlık talebi"

#: models.py:19
msgid "friendship requests"
msgstr "arkadaşlık talepleri"

#: models.py:23
#, python-format
msgid "%(from_user)s wants to be friends with %(to_user)s"
msgstr "%(from_user)s,  %(to_user)s ile arkadaş olmak istiyor"

#: models.py:68
msgid "friendship"
msgstr "arkadaşlık"

#: models.py:69
msgid "friendships"
msgstr "arkadaşlıklar"

#: models.py:72
#, python-format
msgid "%(user)s's friends"
msgstr "%(user)s kullanıcısının arkadaşlıkları"

#: models.py:77
msgid "Friends count"
msgstr "Arkadaş sayısı"

#: models.py:85
msgid "Summary of friends"
msgstr "Arkadaşların özeti"

#: models.py:93
msgid "user blocks"
msgstr "engellemeler"

#: models.py:96
#, python-format
msgid "Users blocked by %(user)s"
msgstr "%(user)s tarafından engellenenler"

#: models.py:101
msgid "Blocks count"
msgstr "Engelleme sayısı"

#: models.py:109
msgid "Summary of blocks"
msgstr "Engellemelerin özeti"

#: views.py:17
msgid "You can't befriend yourself."
msgstr "Kendi kendinizi arkadaş olarak ekleyemezsiniz."

#: views.py:45
#, python-format
msgid "You are already friends with %(user)s"
msgstr "%(user)s ile zaten arkadaşsınız."

#: views.py:105
#, python-format
msgid "You already have an active friend invitation for %(user)s."
msgstr "%(user)s için zaten bir arkadaşlık talebiniz var."

#: views.py:108
#, python-format
msgid "You have requested to be friends with %(user)s."
msgstr "%(user)s ile arkadaş olmayı talep ettiniz."

#: views.py:128
#, python-format
msgid "You are now friends with %(user)s."
msgstr "Artık %(user)s ile arkadaşsınız."

#: views.py:145
#, python-format
msgid "You declined friendship request of %(user)s."
msgstr "%(user)s kullanıcısının arkadaşlık talebini reddettiniz."

#: views.py:148
#, python-format
msgid "%(user)s has declined your friendship request."
msgstr "%(user)s sizin arkadaşlık talebinizi reddetti."

#: views.py:163
#, python-format
msgid "You cancelled your request to be friends with %(user)s."
msgstr "%(user)s için arkadaşlık talebinizi iptal ettiniz."

#: views.py:179
#, python-format
msgid "You are no longer friends with %(user)s."
msgstr "Artık %(user)s ile arkadaş değilsiniz."

#: views.py:182
#, python-format
msgid "%(user)s has removed you as a friend."
msgstr "%(user)s sizi arkadaşlıktan çıkardı."

#: views.py:197
#, python-format
msgid "You have blocked %(user)s."
msgstr "%(user)s kullanıcısını engellediniz."

#: views.py:199
#, python-format
msgid "%(user)s is already blocked."
msgstr "%(user)s zaten engelleniyor."

#: views.py:213
#, python-format
msgid "%(user)s was not blocked."
msgstr "%(user)s zaten engellenmiyordu."

#: views.py:216
#, python-format
msgid "You have unblocked %(user)s."
msgstr "%(user)s için engellemeyi kaldırdınız."

